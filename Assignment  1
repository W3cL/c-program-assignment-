int main() {
    int n, i, j;
    printf("Enter the number of previous terms to add: ");
    scanf("%d", &n);

    int series[n];
    // Initialize the first n-1 terms of the series with 1
    for (i = 0; i < n - 1; i++) {
        series[i] = 1;
    }
    // Calculate the nth term of the series
    series[n - 1] = n;

